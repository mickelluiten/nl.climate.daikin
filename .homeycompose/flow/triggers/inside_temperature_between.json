{
    "id": "inside_temperature_between",
    "title": {
        "en": "The inside temperature is between",
        "nl": "De binnentemperatuur is tussen"
    },
    "args": [{
        "name": "device",
        "type": "device",
        "filter": "driver_id=airairhp"
    }, {
        "name": "inside_temperature_from",
        "type": "range",
        "min": -20,
        "max": 80,
		"step": 0.5,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 1,
        "title": {
            "en": "From",
            "nl": "Van"
        }
    }, {
        "name": "inside_temperature_to",
        "type": "range",
        "min": -20,
        "max": 80,
		"step": 0.5,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 1,
        "title": {
            "en": "To",
            "nl": "Tot"
        }
    }]
}
