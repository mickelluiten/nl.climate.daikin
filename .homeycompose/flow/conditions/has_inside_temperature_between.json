{
    "id": "has_inside_temperature_between",
    "title": {
        "en": "The inside temperature !{{is|isn't}} between",
        "nl": "De binnentemperatuur !{{is|is niet}} tussen"
    },
    "args": [{
        "name": "device",
        "type": "device",
        "filter": "driver_id=airairhp"
    }, {
        "name": "inside_temperature_from",
        "type": "range",
        "min": -10,
        "max": 60,
		"step": 1,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 0,
        "title": {
            "en": "From",
            "nl": "Van"
        }
    }, {
        "name": "inside_temperature_to",
        "type": "range",
        "min": -10,
        "max": 60,
		"step": 1,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 0,
        "title": {
            "en": "To",
            "nl": "Tot"
        }
    }]
}
