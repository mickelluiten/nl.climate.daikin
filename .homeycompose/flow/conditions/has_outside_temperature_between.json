{
    "id": "has_outside_temperature_between",
    "title": {
        "en": "The outside temperature !{{is|isn't}} between",
        "nl": "De buitentemperatuur !{{is|is niet}} tussen"
    },
    "args": [{
        "name": "device",
        "type": "device",
        "filter": "driver_id=airairhp"
    }, {
        "name": "outside_temperature_from",
        "type": "range",
        "min": -50,
        "max": 50,
		"step": 0.5,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 1,
        "title": {
            "en": "From",
            "nl": "Van"
        }
    }, {
        "name": "outside_temperature_to",
        "type": "range",
        "min": -50,
        "max": 50,
		"step": 0.5,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 1,
        "title": {
            "en": "To",
            "nl": "Tot"
        }
    }]
}
