{
    "id": "change_target_temp_by",
    "title": {
        "en": "Change current target temp. by °C",
        "nl": "Wijzig huidige doeltemp. met °C"
    },
    "args": [{
        "name": "device",
        "type": "device",
        "filter": "driver_id=airairhp|airairhphk"
    }, {
        "name": "bytemp",
        "type": "range",
        "min": -5,
        "max": 5,
		"label": "°C",
		"labelMultiplier": 1,
		"labelDecimals": 0,
        "title": {
            "en": "Change target temperature by",
            "nl": "Wijzig de doel temperatuur met"
        }
    }]
}
